{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2019", "WebWorker", "DOM"],
    "strict": true,
    "target": "ES2022",
    "module": "ES2022",
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "outDir": "dist",
    "types": ["svelte"]
  },
  "include": ["src/**/*", "src/*"],
  "exclude": ["node_modules/*", "dist/*"]
}
